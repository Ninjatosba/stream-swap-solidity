    "hardhat:test": "yarn workspace @se-2/hardhat test",
    "hardhat:verify": "yarn workspace @se-2/hardhat verify",
    "postinstall": "husky install",
    "lint": "yarn hardhat:lint",
    "mint-tokens": "yarn workspace @se-2/hardhat hardhat mint-tokens",
    "precommit": "lint-staged",
    "subscribe": "yarn workspace @se-2/hardhat hardhat subscribe",
    "test": "yarn hardhat:test",
    "verify": "yarn hardhat:verify",
    "compile:sync": "yarn workspace @se-2/hardhat compile && yarn workspace @se-2/hardhat sync-abis && yarn workspace @se-2/hardhat generate-indexer-types"
